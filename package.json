{
  "name": "teste-wdio-ios",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.23.3",
    "@wdio/appium-service": "^9.23.3",
    "@wdio/browserstack-service": "9.23.3",
    "@wdio/cli": "^9.23.3",
    "@wdio/local-runner": "^9.23.3",
    "@wdio/mocha-framework": "^9.23.3",
    "@wdio/spec-reporter": "^9.23.3",
    "appium-xcuitest-driver": "^4.4.6"
  },
  "scripts": {
    "wdio": "npm run wdio:ios && npm run wdio:android",
    "wdio:ios": "wdio run ./config/wdio.ios.conf.js",
    "wdio:android": "wdio run ./config/wdio.android.conf.js",
    "allure:report": "allure generate allure-results -c -o allure-report",
    "allure:open": "allure open allure-report",
    "allure": "npm run allure:report && npm run allure:open"
  }
}
